{
	"name": "selfDataflow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "SourceCSV",
						"type": "DatasetReference"
					},
					"name": "Source"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "SinkSnowflake",
						"type": "DatasetReference"
					},
					"name": "Sink"
				}
			],
			"transformations": [
				{
					"name": "filterOnQuatity"
				},
				{
					"name": "groupByOnMonthYear"
				},
				{
					"name": "parse1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          ORDERNUMBER as short,",
				"          QUANTITYORDERED as short,",
				"          PRICEEACH as double,",
				"          ORDERLINENUMBER as short,",
				"          SALES as double,",
				"          ORDERDATE as timestamp,",
				"          STATUS as string,",
				"          QTR_ID as short,",
				"          MONTH_ID as short,",
				"          YEAR_ID as short",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     ignoreNoFilesFound: false,",
				"     moveFiles: ['ADF-class4/datasets/landingZone/sales_data.csv','ADF-class4/datasets/proccessedFiles/']) ~> Source",
				"parse1 filter(QUANTITYORDERED>=30) ~> filterOnQuatity",
				"filterOnQuatity aggregate(groupBy(YEAR_ID,",
				"          MONTH_ID),",
				"     TOTALSALES = sum(SALES)) ~> groupByOnMonthYear",
				"Source parse(column1 = ERROR_FUNCTION(''),",
				"     format: 'json',",
				"     documentForm: 'documentPerLine') ~> parse1",
				"groupByOnMonthYear sink(allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     input(",
				"          YEAR_ID as string,",
				"          MONTH_ID as string,",
				"          TOTALSALES as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     stageInsert: true) ~> Sink"
			]
		}
	}
}