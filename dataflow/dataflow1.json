{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "SourceCSV",
						"type": "DatasetReference"
					},
					"name": "Source"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "filterOnQuatity"
				}
			],
			"scriptLines": [
				"source(output(",
				"          ORDERNUMBER as string,",
				"          QUANTITYORDERED as string,",
				"          PRICEEACH as string,",
				"          ORDERLINENUMBER as string,",
				"          SALES as string,",
				"          ORDERDATE as string,",
				"          STATUS as string,",
				"          QTR_ID as string,",
				"          MONTH_ID as string,",
				"          YEAR_ID as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     ignoreNoFilesFound: false,",
				"     moveFiles: ['ADF-class4/datasets/landingZone/sales_data.csv','ADF-class4/datasets/processedFiles']) ~> Source",
				"Source filter(QUANTITYORDERED>=30) ~> filterOnQuatity"
			]
		}
	}
}