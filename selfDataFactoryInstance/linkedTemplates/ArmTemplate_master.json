{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "selfDataFactoryInstance"
		},
		"SnowflakeLinkedService_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SnowflakeLinkedService'"
		},
		"blobLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'blobLinkedService'"
		},
		"SnowflakeLinkedService_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sales_mart"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "selfDataFactoryInstance_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"SnowflakeLinkedService_password": {
						"value": "[parameters('SnowflakeLinkedService_password')]"
					},
					"blobLinkedService_connectionString": {
						"value": "[parameters('blobLinkedService_connectionString')]"
					},
					"SnowflakeLinkedService_properties_typeProperties_database": {
						"value": "[parameters('SnowflakeLinkedService_properties_typeProperties_database')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}